# Environment Configuration for BARNBOWL PremPredictions
# Copy this file to .env.local and fill in your values

REACT_APP_ENVIRONMENT=development
REACT_APP_APP_NAME=BARNBOWL PremPredictions
REACT_APP_VERSION=1.0.0
REACT_APP_API_BASE_URL=http://localhost:5001

# Optional: API Keys for live data
REACT_APP_FOOTBALL_DATA_KEY=your_football_data_api_key
REACT_APP_ODDS_API_KEY=your_odds_api_key
REACT_APP_RAPID_API_KEY=your_rapid_api_key

# Analytics
REACT_APP_ENABLE_ANALYTICS=false